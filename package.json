{
	"name": "py-wrapper",
	"displayName": "Python Wrapper",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Formatters"
	],
	"activationEvents": [
		"onCommand:py-wrapper.wrap-at-cursor"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
			"command": "py-wrapper.wrap-at-cursor",
			"title": "Py Wrapper: Wrap statement at Cursor"
		}]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"diff-match-patch": "^1.0.0"
	},
	"devDependencies": {
        "@types/diff-match-patch": "^1.0.32",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
